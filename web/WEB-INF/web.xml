<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>/Token</servlet-name>
        <servlet-class>qiniu.demo.service.GetTokenServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>/Token</servlet-name>
        <url-pattern>/token</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>/List</servlet-name>
        <servlet-class>qiniu.demo.service.FileManageServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>/List</servlet-name>
        <url-pattern>/list</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>/Delete</servlet-name>
        <servlet-class>qiniu.demo.service.DeleteFileServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>/Delete</servlet-name>
        <url-pattern>/delete</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>/GetStatus</servlet-name>
        <servlet-class>qiniu.demo.service.GetFileStat</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>/GetStatus</servlet-name>
        <url-pattern>/getStatus</url-pattern>
    </servlet-mapping>
</web-app>